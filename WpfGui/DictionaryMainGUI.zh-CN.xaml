<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib"
                    xmlns:wnd="clr-namespace:System.Windows;assembly=PresentationFramework">
    
    <sys:Double x:Key="WndHeight">300</sys:Double>
    <sys:Double x:Key="WndWidth">420</sys:Double>

    <sys:String x:Key="PageSizeBox">页面尺寸</sys:String>
    <sys:String x:Key="PageSizeSameWithEach">与每张图片一致</sys:String>
    <sys:String x:Key="PageSizeWidthFixed">固定宽度</sys:String>
    <sys:String x:Key="PageSizeSizeFixed">固定大小</sys:String>
    <sys:String x:Key="PageSizeUseFirstImage">使用第一张图片的尺寸数据</sys:String>
    <sys:String x:Key="PageSizeWidth">宽度</sys:String>
    <sys:String x:Key="PageSizeHeight">高度</sys:String>

    <sys:String x:Key="IOBox">输入输出</sys:String>
    <sys:String x:Key="IORecursion">递归输入</sys:String>
    <sys:String x:Key="IOKeepStructure">保持结构</sys:String>
    <sys:String x:Key="IOCompress">全部压缩</sys:String>
    <sys:String x:Key="IOStayNoMove">输出到原位</sys:String>
    <sys:Double x:Key="IOBoxHeight">60</sys:Double>
    <wnd:Thickness x:Key="IOBoxChk1">10,12,0,0</wnd:Thickness>
    <wnd:Thickness x:Key="IOBoxChk2">95,12,0,0</wnd:Thickness>
    <wnd:Thickness x:Key="IOBoxChk3">180,12,0,0</wnd:Thickness>
    <wnd:Thickness x:Key="IOBoxChk4">265,12,0,0</wnd:Thickness>

    <sys:String x:Key="Ready">就绪</sys:String>
    <sys:String x:Key="HaveFinishedPercent">已完成 {0:F2}%</sys:String>
    <sys:String x:Key="Error">错误</sys:String>
    <sys:String x:Key="Warning">警告</sys:String>
    <sys:String x:Key="InvalidDrop">拖入的数据不合规。</sys:String>
    <sys:String x:Key="WaitForCurrentTask">请等待当前任务完成。</sys:String>

    <sys:String x:Key="CannotProcess">以下内容无法处理：</sys:String>
    <sys:String x:Key="CannotMerge">以下文件无法加入“{0}”：</sys:String>

</ResourceDictionary>