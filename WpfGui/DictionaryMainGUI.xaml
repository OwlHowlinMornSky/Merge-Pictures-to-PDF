<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <sys:String x:Key="PageSizeBox">Page Size</sys:String>
    <sys:String x:Key="PageSizeWidthFixed">Fixed width</sys:String>
    <sys:String x:Key="PageSizeHeightFixed">Fixed height</sys:String>
    <sys:String x:Key="PageSizePaperCustom">Custom</sys:String>

    <sys:String x:Key="IOBox">Input/Output</sys:String>
    <sys:String x:Key="IORecursion">Recurse input folder</sys:String>
    <sys:String x:Key="IOKeepStructure">Keep folder structure</sys:String>
    <sys:String x:Key="IOCompress">Compress every image</sys:String>
    <sys:String x:Key="IOStayNoMove">Output to same folder as input</sys:String>

    <sys:String x:Key="DetailButton">More Preferences</sys:String>

    <sys:String x:Key="Ready">Ready</sys:String>
    <sys:String x:Key="HaveFinishedPercent">{0:F2}% finished</sys:String>
    <sys:String x:Key="Error">Error</sys:String>
    <sys:String x:Key="Warning">Warning</sys:String>
    <sys:String x:Key="InvalidDrop">Data dropped is invalid.</sys:String>
    <sys:String x:Key="WaitForCurrentTask">Please wait for task finished.</sys:String>

    <sys:String x:Key="CannotProcess" xml:space="preserve"
>Some files cannot be processed.
Check log file for details:
{0}</sys:String>
    <sys:String x:Key="ChooseDestinationDir">Choose output place</sys:String>
    <sys:String x:Key="ChooseMoveDestinationDir">Choose move destination</sys:String>

    <sys:String x:Key="DetailTitle">More Preferences</sys:String>
